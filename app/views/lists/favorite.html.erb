<section class="list-list">
  <div class="row">
    <div class="col-md-offset-3 col-md-6 col-sm-12 text-center">
      <h1>Favorite lists</h1>
    </div>
  </div>

  <div class="row">
    <div class="col-md-offset-3 col-md-6 col-sm-12 text-center">
      <div class="table-responsive">
        <table class="table">
          <thead>
            <th>Name</th>
          </thead>
          <tbody>
            <%= render @lists %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</section>
